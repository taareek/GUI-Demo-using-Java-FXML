<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fxmlpractice.FXMLDocumentController">
   <children>
      <VBox fx:id="orderFood" layoutX="32.0" layoutY="21.0" prefHeight="333.0" prefWidth="254.0" spacing="15.0">
         <children>
            <Label fx:id="dinnerMenu" text="Dinner menu">
               <font>
                  <Font name="System Bold Italic" size="28.0" />
               </font>
            </Label>
            <CheckBox fx:id="chickenCheckbox" mnemonicParsing="false" text="Chicken " />
            <CheckBox fx:id="beefCheckbox" mnemonicParsing="false" text="Beef" />
            <CheckBox fx:id="fishCheckbox" mnemonicParsing="false" text="Fish" />
            <Button fx:id="orderFoodButton" mnemonicParsing="false" onAction="#orderFoodButtonOnClick" text="order food" />
            <Label fx:id="orderFoodLabel" alignment="TOP_LEFT" prefHeight="96.0" prefWidth="137.0" />
         </children>
      </VBox>
      <VBox alignment="TOP_CENTER" layoutX="286.0" layoutY="28.0" prefHeight="270.0" prefWidth="261.0" spacing="15.0">
         <children>
            <Label text="Choice Box">
               <font>
                  <Font name="Comic Sans MS Bold" size="18.0" />
               </font>
            </Label>
            <ChoiceBox fx:id="choiceBox" prefWidth="150.0" />
            <Button fx:id="buttonClick" mnemonicParsing="false" onAction="#choiceBoxOnAction" text="Favorite" />
            <Label fx:id="choiceBoxLabel" prefHeight="63.0" prefWidth="155.0" text=" " />
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
</AnchorPane>
